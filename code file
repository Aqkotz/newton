
import math

Function = input("Input Function: ")
IntvlS = int(input("Input Interval Start: "))
IntvlE = int(input("Input Interval End: "))
h = 0.0000001

i = (float(IntvlS)-float(IntvlE))/2+float(IntvlS)
lastx = i
index = 0
maxindex = 50
found = False
while found == False:
    if index <= maxindex:
        print(i)
        x = i
        FunCat = eval(Function)
        x = i + h
        FunDog = eval(Function)
        FunPets = FunCat-FunDog
        FunTimes = FunPets/h
        print(FunTimes)
        x = i-FunDog/FunTimes
        if (x-lastx) > 0.1 and (x-lastx) < 0.1:
            found = True
            print(x)
        i = x
    else:
        found = True
        print ("couldn't do it")
    index += 1